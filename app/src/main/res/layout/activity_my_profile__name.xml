<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_profile__name"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@mipmap/setupbg"
    android:layout_height="match_parent"
    tools:context="com.erbao.joystar.moudule.home.activity.MyProfile_NameActivity">


    <include layout="@layout/handview1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        ></include>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/lin_size"
        android:background="@drawable/bg_white"
        >
        <EditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="15dp"
            android:id="@+id/profile_edts"
            android:hint="@string/Please_enter"
            android:textColorHint="@color/color_Gray"
            android:paddingLeft="15dp"
            android:background="@null"
            android:textColor="@color/color_Blackfont"
            android:textColorHighlight="@color/color_Gray"
            />


    </LinearLayout>



</LinearLayout>
