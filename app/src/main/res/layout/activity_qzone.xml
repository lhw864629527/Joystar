<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_qzone"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.erbao.joystar.moudule.qzone.activity.QzoneActivity">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:id="@+id/qzone_img"
        android:src="@mipmap/toumingw" />


    <com.erbao.joystar.views.AutoListView
        android:id="@+id/qzone_listv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@null"
        android:scrollbars="none">


    </com.erbao.joystar.views.AutoListView>


</RelativeLayout>
